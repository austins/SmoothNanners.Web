@model Blazicon
@{
    var attributes = new Dictionary<string, string?>(Model.Icon.Attributes)
    {
        ["width"] = "1em",
        ["height"] = "1em"
    };
}

<svg class="align-middle" @Html.Raw(string.Join(' ', attributes.Select(kvp => $"{kvp.Key}=\"{kvp.Value}\"")))>
    @Html.Raw(Model.Icon.Content)
</svg>
