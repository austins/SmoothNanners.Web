@model SvgIcon
@{
    var attributes = new Dictionary<string, string?>(Model.Attributes)
    {
        ["width"] = "1em",
        ["height"] = "1em"
    };
}

<svg class="align-middle" aria-hidden="true"
     @Html.Raw(string.Join(' ', attributes.Select(kvp => $"{kvp.Key}=\"{kvp.Value}\"")))>
    @Html.Raw(Model.Content)
</svg>
